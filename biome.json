{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"a11y": {
				"noSvgWithoutTitle": "off",
				"useMediaCaption": "off",
				"useKeyWithClickEvents": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			},
			"style": {
				"noNonNullAssertion": "off",
				"noParameterAssign": "off"
			},
			"performance": {
				"noDelete": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
